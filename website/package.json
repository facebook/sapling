{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ci": "yarn lint && yarn format:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "typecheck": "tsc",
    "signsource": "node ./scripts/signsource.js"
  },
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@mdx-js/react": "^1.6.22",
    "async-mutex": "^0.4.0",
    "clsx": "^1.2.1",
    "deep-equal": "^2.1.0",
    "docusaurus-lunr-search": "^2.2.0",
    "docusaurus-plugin-internaldocs-fb": "1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-simple-code-editor": "^0.13.1",
    "remark-github": "^10.1.0",
    "sapling-output-plugin": "file:src/plugins/sapling-output"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@docusaurus/module-type-aliases": "2.1.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "signedsource": "2.0.0",
    "stylelint": "^15.10.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
