load("//fbpkg:fbpkg.bzl", "fbpkg")

oncall("scm_server_infra")

fbpkg.builder(
    name = "scm.scsc",
    buck_opts = fbpkg.buck_opts(
        config = {"fbcode.dwp": "true"},
        mode = "opt",
    ),
    fail_on_redundant_configerator_fbpkg = False,
    path_actions = {
        "scsc": "fbcode//eden/mononoke/scs/client:scsc",
    },
)
