load("@fbcode_macros//build_defs:rust_library.bzl", "rust_library")

oncall("mercurial_release")

rust_library(
    name = "minibench",
    srcs = glob(["src/**/*.rs"]),
    autocargo = {"cargo_toml_config": {"package": {
        "autoexamples": False,
        "license": "MIT",
    }}},
    crate_root = "src/lib.rs",
)
