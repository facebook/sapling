    name = "staticconfig",
    srcs = glob(["src/**/*.rs"]),
    crate_root = "src/lib.rs",
    deps = [
        "fbsource//third-party/rust:phf",
        "//eden/scm/lib/config/model:configmodel",
        "//eden/scm/lib/config/static/macros:staticconfig_macros",
    ],
)
