    name = "test",
    srcs = glob(["*Test.cpp"]),
    supports_static_listing = False,
    deps = [
        "//eden/fs/journal:journal",
        "//eden/fs/model:model",
        "//folly/portability:gmock",
        "//folly/portability:gtest",
    ],
)
