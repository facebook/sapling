    name = "rocksdb",
    srcs = glob(["*.cpp"]),
    headers = glob(["*.h"]),
    deps = [
        "//folly/logging:logging",
    ],
    exported_deps = [
        "fbsource//third-party/fmt:fmt",
        "//folly:likely",
        "//rocksdb:rocksdb",
    ],
)
